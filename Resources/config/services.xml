<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="presto_heads.name_case_lib.inflector.class">PrestoHeads\NameCaseLibBundle\Bridge\Inflector</parameter>
    </parameters>

    <services>
        <service id="prestoheads.name_case_lib.inflector" class="%presto_heads.name_case_lib.inflector.class%">
        </service>

        <service id="ph.inflector" alias="prestoheads.name_case_lib.inflector" />
    </services>
</container>
